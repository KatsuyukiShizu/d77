$comment
Input file for calculating transition dipole moment density 
between S0 and S2 
| 0 > : ground state (S0)
| 6 > : second lowest excited singlet state (S2)
$end_comment

$control
property      = dipole
runtyp        = density
method        = td
save_rho_cube = yes
$end_control

$elec_state
bra = < 0 |
ket = | 6 >
$end_elec_state

$grid
xmin = -5.0
xmax =  5.0
ymin = -7.0
ymax =  7.0
zmin = -5.0
zmax =  5.0
dx   = 0.20
dy   = 0.20
dz   = 0.20
$end_grid

DIR_INP_ELEC = ../INP_ELEC
